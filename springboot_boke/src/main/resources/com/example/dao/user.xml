<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.demo.dao.Userdao.">
    <insert id="add" parameterType="com.example.demo.dao.Userdao">
        insert  into t_user(id,username,password,email,vaild)values(#{id},#{username},#{password},#{email},#{vaild})
    </insert>



</mapper>